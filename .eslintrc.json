{
  "parser": "babel-eslint",
  "plugins": [
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true
  },
  "globals": {
  },
  "ecmaFeatures": {
  },
  "rules": {
    // Strict mode
    "strict": ["error", "never"],

    "no-unused-vars": 0,
    "no-path-concat": 0,

    // Code style
    "indent": ["error", "tab"],
    "quotes": ["error", "single"]
  }
}
